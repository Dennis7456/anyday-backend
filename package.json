{
  "name": "anyday-backend-backup",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --config jest.config.ts ./__tests__",
    "test:watch": "NODE_ENV=test jest --config jest.config.ts ./__tests__ --watch --verbose --detectOpenHandles",
    "test:staged": "NODE_ENV=test jest --findRelatedTests",
    "test:cache": "npx jest --clearCache",
    "dev": "ts-node-dev --exit-child src/index.ts",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate:prisma && tsc",
    "generate:prisma": "prisma generate",
    "start": "ts-node dist/src/index.js",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,graphql}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,graphql}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "validate": "npm -s run format:check && npm -s run lint && npm -s run format && npm -s run lint:fix"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run lint",
      "npm run format",
      "npm run test:staged"
    ],
    "src/**/*.graphql": [
      "prettier --write"
    ],
    "src/**/*.json": [
      "prettier --write"
    ]
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@apollo/client": "^3.11.10",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.14.0",
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/fastify-multipart": "^0.7.2",
    "@types/google-cloud__storage": "^2.3.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mock-fs": "^4.13.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "apollo-server-testing": "^2.25.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "graphql": "^15.9.0",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^15.2.10",
    "mock-fs": "^5.4.1",
    "nock": "^13.5.6",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^8.14.0"
  },
  "dependencies": {
    "@fastify/formbody": "^8.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/static": "^8.0.2",
    "@google-cloud/storage": "^7.14.0",
    "@graphql-tools/schema": "^10.0.7",
    "@types/express": "^5.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.0",
    "apollo-server": "^3.13.0",
    "bcryptjs": "^2.4.3",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "fast-csv": "^5.0.2",
    "fastify": "^5.1.0",
    "fastify-multer": "^2.0.3",
    "fastify-multipart": "^5.4.0",
    "fastify-raw-body": "^5.0.0",
    "fluent-json-schema": "^5.0.0",
    "graphql-helix": "^1.13.0",
    "graphql-import-node": "^0.0.5",
    "graphql-request": "^7.1.2",
    "graphql-scalars": "^1.23.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-mailjet": "^6.0.6",
    "redis": "^4.7.0",
    "stripe": "^17.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "uuid": "^11.0.3"
  }
}
